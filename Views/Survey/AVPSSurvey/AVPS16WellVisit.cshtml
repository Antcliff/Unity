                        </li>
                        @if (Model.HasMenB)
                        {
                        <li>                           
                            Meningococcal B vaccine? @Html.TextBoxFor(m => m.MenBRecCt, new { @class = "form-control Width300px" }) @Html.ValidationMessageFor(m => m.MenBRecCt)
                        </li>
                        }
