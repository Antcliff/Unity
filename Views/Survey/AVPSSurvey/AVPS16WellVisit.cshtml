                                        <div id="Receptionist">
                                            <b>Receptionist</b>
                                            <br />
                                            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;@Html.CheckBoxFor(m => m.VaccComm_Receptionist_Provides) Provides written information
                                            <br />
                                            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;@Html.CheckBoxFor(m => m.VaccComm_Receptionist_Discusses) Discusses vaccines due
                                            <br />
                                            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;@Html.CheckBoxFor(m => m.VaccComm_Receptionist_Answers) Answers vaccine questions
                                            <br />
                                            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;@Html.CheckBoxFor(m => m.VaccComm_Receptionist_Recommends) Recommends vaccine
                                        </div>
                                        <div id="MedicalAssistant">
